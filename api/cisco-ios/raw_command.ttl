message = ''
sendln ''
sendln command
sprintf '%s.*%s' prompt_re command
waitregex inputstr
do
  wait prompt #13'% '
  if result = 1 then
    break
  elseif result = 2 then
    message = '(IOS) command error: '
    strconcat message command
    include 'lib\sys\log.ttl'
    recvln
    message = inputstr
    include 'lib\sys\log.ttl'
  endif
loop while 1
sendln ''
